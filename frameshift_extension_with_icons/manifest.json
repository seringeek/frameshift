{
  "manifest_version": 3,
  "name": "FrameShift",
  "description": "A minimalist new tab with mental models and Pomodoro timer for focused thinking.",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [],
  "action": {
    "default_icon": "icons/icon48.png"
  }
}